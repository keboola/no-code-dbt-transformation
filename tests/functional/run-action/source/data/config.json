{
  "authorization": {
    "workspace": {
      "host": "%env(string:SNOWFLAKE_HOST)%",
      "warehouse": "%env(string:SNOWFLAKE_WAREHOUSE)%",
      "database": "%env(string:SNOWFLAKE_DATABASE)%",
      "schema": "%env(string:SNOWFLAKE_SCHEMA)%",
      "user": "%env(string:SNOWFLAKE_USER)%",
      "password": "%env(string:SNOWFLAKE_PASSWORD)%"
    }
  },
  "parameters": {
    "models": [
      "SELECT * FROM {{ source('in.c-Google_Insights', 'audience_overview') }}",
      "SELECT \"id\", \"idProfile\", \"date\", REPLACE(\"language\", 'en-us', 'EN') as \"language\", \"deviceCategory\", \"city\" FROM {{ ref('model1') }}",
      "SELECT * FROM {{ ref('model2') }} WHERE \"deviceCategory\" = 'desktop'",
      "SELECT count(\"id\") as \"count\" FROM {{ ref('model3') }} GROUP BY \"city\""
    ]
  }
}
